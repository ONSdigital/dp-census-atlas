{
  "name": "dp-census-atlas",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "npx tsc --noEmit",
    "dev": ". env/dev.sh && vite dev",
    "build": "vite build",
    "build:dev": ". env/dev.sh && npm run build",
    "build:netlify": ". env/netlify.sh && npm run build",
    "build:ons": ". env/ons.sh && npm run build",
    "preview": "vite preview",
    "test": ". env/test.sh && vitest run src/",
    "e2e": "playwright test e2e/",
    "e2e:ci": "start-server-and-test dev http://localhost:28100 e2e",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/adapter-netlify": "^1.0.5",
    "@sveltejs/adapter-node": "^1.1.0",
    "@sveltejs/kit": "^1.2.9",
    "@turf/bbox": "^6.5.0",
    "@types/jest": "^29.1.1",
    "@types/mapbox-gl": "^2.7.6",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.12",
    "dotenv": "^16.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^29.1.2",
    "playwright": "^1.27.1",
    "postcss": "^8.4.17",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "start-server-and-test": "^1.14.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.2",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  },
  "type": "module",
  "dependencies": {
    "@mapbox/tilebelt": "^1.0.2",
    "@mapbox/vector-tile": "^1.3.1",
    "@square/svelte-store": "^1.0.1",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/boolean-intersects": "^6.5.0",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "d3-dsv": "^3.0.1",
    "mapbox-gl": "^1.13.2",
    "pbf": "^3.2.1",
    "pym.js": "^1.3.2",
    "rxjs": "^7.5.7",
    "svelte-select": "^4.4.7",
    "tippy.js": "^6.3.7",
    "topojson-client": "^3.1.0"
  }
}
