{
  "name": "dp-census-atlas",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "tsc --noEmit",
    "serve-local-content-jsons": "http-server scripts/content/output_content_jsons -p 8090 --cors",
    "dev": "npm run serve-local-content-jsons & . env/dev.sh && vite dev",
    "build": "vite build",
    "build:dev": ". env/dev.sh && npm run build",
    "build:netlify": ". env/netlify.sh && npm run build",
    "build:ons": ". env/ons.sh && npm run build && ./workaround-sveltekit-3726.sh",
    "preview": "vite preview",
    "test": ". env/test.sh && vitest run",
    "test:ui": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@mapbox/tilebelt": "^1.0.2",
    "@mapbox/vector-tile": "^1.3.1",
    "@playwright/test": "^1.25.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.96",
    "@sveltejs/kit": "next",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@types/jest": "^29.1.1",
    "@types/mapbox-gl": "^2.7.6",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.12",
    "dotenv": "^16.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.1.2",
    "pbf": "^3.2.1",
    "postcss": "^8.4.17",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.4"
  },
  "type": "module",
  "dependencies": {
    "@square/svelte-store": "^1.0.1",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/boolean-intersects": "^6.5.0",
    "d3-dsv": "^3.0.1",
    "mapbox-gl": "^1.13.2",
    "pym.js": "^1.3.2",
    "rxjs": "^7.5.7",
    "svelte-i18n": "^3.4.0",
    "svelte-select": "^4.4.7"
  }
}
